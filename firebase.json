{
  "hosting": [
    {
      "target": "default",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "amritdash",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ],
  "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
}
